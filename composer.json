{
    "name": "torralbodavid/image-optimizer",
    "description": "Image Optimizer will help you to take your images to the next level",
    "keywords": [
        "torralbodavid",
        "image-optimizer"
    ],
    "homepage": "https://github.com/torralbodavid/image-optimizer",
    "license": "MIT",
    "authors": [
        {
            "name": "David Torralbo",
            "email": "me@davidtorralbo.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^8.37|^9.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "nunomaduro/collision": "^5.3",
        "nunomaduro/larastan": "^1.0.2",
        "orchestra/testbench": "^6.23.2|^7.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Torralbodavid\\ImageOptimizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Torralbodavid\\ImageOptimizer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
